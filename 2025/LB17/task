#include <iostream>    // Підключаємо бібліотеку для введення/виведення (cout, cin)
#include <cmath>       // Підключаємо бібліотеку для математичних операцій (sqrt)

using namespace std;   // Щоб не писати std:: перед cout, endl тощо

     // Оголошення функції, яка перевіряє, чи є число простим
bool isPrime(int num) {    // Функція повертає true, якщо число просте, інакше false
    if (num < 2)           // Простими вважаються числа тільки від 2 і більше
        return false;       // Якщо число менше 2, воно не просте — повертаємо false

        // Перевіряємо, чи має число дільники від 2 до квадратного кореня з числа
    for (int i = 2; i <= sqrt(num); i++) {     // Перевірка дільників: i змінюється від 2 до sqrt(num)
        if (num % i == 0)                     // Якщо остача від ділення дорівнює 0, тобто num ділиться націло на i
            return false;                     // Число має дільник => не є простим => повертаємо false
    }
    return true;      // Якщо дільників не знайшли — число просте => повертаємо true
}

// Головна функція програми
int main() {
    // Проходимо всі числа від 0 до 21 включно
    for (int i = 0; i <= 21; i++) {
        if (isPrime(i))           // Якщо число i є простим (функція повертає true)
            cout << i << " ";     // Виводимо число на екран з пробілом
    }

    cout << endl; // Перехід на новий рядок після завершення циклу
    return 0;     // Повернення 0 — успішне завершення програми
}
