#include <iostream> // Для виводу в консоль
using namespace std;

// Функція перевіряє, чи рік високосний
bool isLeap(int year) {
    // Логіка:
    // - Рік високосний, якщо ділиться на 4
    // - Але НЕ високосний, якщо ділиться на 100
    // - Але якщо ділиться на 400 — тоді високосний (виняток)
    return (year % 4 == 0 && year % 100 != 0) || (year % 400 == 0);
    // year % 4 == 0   → рік ділиться на 4 (може бути високосним)
    // year % 100 != 0 → але не має ділитися на 100
    // year % 400 == 0 → або якщо ділиться на 400, тоді точно високосний
}

int main(void) {
    for(int yr = 1995; yr < 2017; yr++) {
        // Виводимо рік і результат: 1 — високосний, 0 — не високосний
        cout << yr << " -> " << isLeap(yr) << endl;
    }
    return 0;
}
