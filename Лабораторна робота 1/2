#include <iostream>
#include <math.h> 

int main() {
   
  double a;
   double b;
   
    std::cout << "a is = ";
    std::cin >> a;
    std::cout << "b is = ";
    std::cin >>b;
    
    const double numerator = 0.137;
    
    double result = std::cbrt((a - b) / (b + a) * std::exp(std::log(std::cos((a - b) * M_PI / 8)) / numerator));
    
    std::cout << "The result is: " << result << std::endl;
    
    return 0;
}
