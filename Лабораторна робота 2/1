#include <iostream>
#include <cmath>

using namespace std;

int main() {
    double x, y;

   
    cout << "Enter x: ";
    cin >> x;
    cout << "Enter  y: ";
    cin >> y;

   
    double result;
    if (x <= 0) {
        result = pow(x, 2) - sin(y);
    } else {
        result = sqrt(x) + cos(y);
    }

    cout << "Value Y: " <<result << std::endl;
}
